
.container2{
  font-size: 30rpx;
}
.container2 .dls{
  display: flex;
  margin: 20rpx 0;
}
.container2 .dls dl{
  margin: 10rpx  50rpx;
}
.container2 dl dt{
  width: 80rpx;
  height: 80rpx;
  display:block;
}
.container2 dl dt image{
   width: 80rpx;
