}
.look{
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.look .one{
  font-size: 35rpx;
  font-weight: 800;
}
.look .two{
  color:rgb(167, 163, 163);
}
